{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "compile": "tsc -p .",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "yarn-i": "yarn install",
    "pod-install": "cd ios && pod install && cd ..",
    "jetify": "npx jetify",
    "setup": "npm run yarn-i && npm run jetify && npm run pod-install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --no-error-on-unmatched-pattern './src/**/*.{js,jsx,ts,tsx}'",
    "format": "npm run lint --fix",
    "pretty": "prettier --write --no-error-on-unmatched-pattern './src/**/*.{js,jsx,ts,tsx}'",
    "pretty-Lint": "npm run pretty && npm run format && npm run lint"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "react": "^17.0.2",
    "react-native": "^0.66.3",
    "react-native-mmkv": "^2.4.1",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.0.10",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.4.6",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.7",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@types/react": "^18.0.10"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}