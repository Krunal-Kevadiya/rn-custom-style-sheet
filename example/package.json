{
  "name": "rn-custom-style-sheet-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Plus'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ios-air": "react-native run-ios  --simulator=\"iPad Air (5th generation)\"",
    "ios-12": "react-native run-ios  --simulator=\"iPad Pro (12.9-inch) (5th generation)\"",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace RnCustomStyleSheetExample.xcworkspace -scheme RnCustomStyleSheetExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "patch-package": "^6.4.7",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-mmkv": "^2.11.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^27.4.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.7",
    "metro-react-native-babel-preset": "^0.67.0",
    "pod-install": "^0.1.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}